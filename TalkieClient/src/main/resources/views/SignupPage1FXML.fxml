<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane 
   maxHeight="-Infinity" maxWidth="-Infinity" 
   minHeight="-Infinity" minWidth="-Infinity" 
   prefHeight="700.0" prefWidth="1000.0" 
   stylesheets="@loginStyle.css" 
   xmlns="http://javafx.com/javafx/19" 
   xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="gov.iti.jets.controllers.SignupPageFxmlController">
   <center>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" opacity="0.8" prefHeight="607.0" prefWidth="658.0" style="-fx-background-color: #ffffff;" BorderPane.alignment="CENTER">
         <center>
            <VBox prefHeight="535.0" prefWidth="658.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Welcom in Talkie" textFill="#549bad">
                     <font>
                        <Font name="Lucida Calligraphy Italic" size="31.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="185.0" right="185.0" top="15.0" />
                     </VBox.margin>
                  </Label>
                  <HBox>
                     <VBox.margin>
                        <Insets bottom="5.0" left="20.0" top="40.0" />
                     </VBox.margin>
                     <children>
                        <VBox prefHeight="62.0" prefWidth="294.0">
                           <children>
                              <Label text="Name :">
                                 <font>
                                    <Font name="Lucida Calligraphy Italic" size="17.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="nameTextField" prefHeight="35.0" prefWidth="390.0" promptText="Enter your name">
                                 <VBox.margin>
                                    <Insets left="30.0" right="30.0" />
                                 </VBox.margin>
                              </TextField>
                           </children>
                        </VBox>
                        <VBox layoutX="10.0" layoutY="10.0" prefHeight="62.0" prefWidth="285.0">
                           <children>
                              <Label text="Phone number :">
                                 <font>
                                    <Font name="Lucida Calligraphy Italic" size="17.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="phoneNoTextField" prefHeight="35.0" prefWidth="390.0" promptText="Enter your phone number">
                                 <VBox.margin>
                                    <Insets left="30.0" right="30.0" />
                                 </VBox.margin>
                              </TextField>
                           </children>
                           <HBox.margin>
                              <Insets left="30.0" />
                           </HBox.margin>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox layoutX="30.0" layoutY="112.0">
                     <children>
                        <VBox prefHeight="62.0" prefWidth="294.0">
                           <children>
                              <Label text="Password:">
                                 <font>
                                    <Font name="Lucida Calligraphy Italic" size="17.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="passwordTextField" prefHeight="35.0" prefWidth="390.0" promptText="Enter your password">
                                 <VBox.margin>
                                    <Insets left="30.0" right="30.0" />
                                 </VBox.margin>
                              </TextField>
                           </children>
                        </VBox>
                        <VBox layoutX="10.0" layoutY="10.0" prefHeight="62.0" prefWidth="285.0">
                           <children>
                              <Label text="Confirm password">
                                 <font>
                                    <Font name="Lucida Calligraphy Italic" size="17.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="ConfirmPasswordTextField" prefHeight="35.0" prefWidth="390.0" promptText="Enter your password again">
                                 <VBox.margin>
                                    <Insets left="30.0" right="30.0" />
                                 </VBox.margin>
                              </TextField>
                           </children>
                           <HBox.margin>
                              <Insets left="30.0" />
                           </HBox.margin>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets left="20.0" top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="30.0" layoutY="220.0">
                     <children>
                        <VBox prefHeight="62.0" prefWidth="294.0">
                           <children>
                              <Label text="Email:">
                                 <font>
                                    <Font name="Lucida Calligraphy Italic" size="17.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="emailTextField" prefHeight="35.0" prefWidth="390.0" promptText="Enter your email">
                                 <VBox.margin>
                                    <Insets left="30.0" right="30.0" />
                                 </VBox.margin>
                              </TextField>
                           </children>
                        </VBox>
                        <VBox layoutX="10.0" layoutY="10.0" prefHeight="62.0" prefWidth="285.0">
                           <children>
                              <Label text="Country:">
                                 <font>
                                    <Font name="Lucida Calligraphy Italic" size="17.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="countryTextField" prefHeight="35.0" prefWidth="390.0" promptText="Enter your country">
                                 <VBox.margin>
                                    <Insets left="30.0" right="30.0" />
                                 </VBox.margin>
                              </TextField>
                           </children>
                           <HBox.margin>
                              <Insets left="30.0" />
                           </HBox.margin>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets left="20.0" top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="30.0" layoutY="292.0">
                     <children>
                        <VBox prefHeight="62.0" prefWidth="294.0">
                           <children>
                              <Label text="Birthdate:">
                                 <font>
                                    <Font name="Lucida Calligraphy Italic" size="17.0" />
                                 </font>
                              </Label>
                              <DatePicker fx:id="datePicker" prefHeight="35.0" prefWidth="235.0">
                                 <VBox.margin>
                                    <Insets left="30.0" />
                                 </VBox.margin>
                              </DatePicker>
                           </children>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets left="20.0" top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="30.0" layoutY="364.0">
                     <children>
                        <VBox prefHeight="62.0" prefWidth="294.0">
                           <children>
                              <Label text="Gender:">
                                 <font>
                                    <Font name="Lucida Calligraphy Italic" size="17.0" />
                                 </font>
                              </Label>
                              <HBox prefHeight="100.0" prefWidth="200.0">
                                 <children>
                                    <ButtonBar>
                                       <buttons>
                                          <RadioButton fx:id="maleRadioButton" mnemonicParsing="false" text="Male" />
                                          <RadioButton fx:id="FemaleRadioButton" mnemonicParsing="false" text="Female" />
                                       </buttons>
                                    </ButtonBar>
                                 </children>
                                 <VBox.margin>
                                    <Insets left="20.0" />
                                 </VBox.margin>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets left="20.0" top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <Button fx:id="startButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" style="-fx-background-color: #3C4C70;" text="Let's Start" textFill="WHITE">
                     <padding>
                        <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="450.0" right="50.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Label id="labelp" fx:id="previousLabel" text="Previous page" textFill="#258096" underline="true">
                     <font>
                        <Font name="Lucida Calligraphy Italic" size="13.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" top="20.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </center>
</BorderPane>
